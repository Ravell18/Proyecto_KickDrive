@page
@model EnergyUI.Pages.Containers.Index4Model
@{
    ViewData["Title"] = "Index4";
}

<h1 class="text-info">Agregar Vehiculos</h1>
<form class="mt-3" method="post">
</form>
<form>
        <div class="form-inline">
            <label for="inputEmail4">Fecha </label>
            <input type="time" class="form-control mb-2 mr-sm-2" id="inputEmail4">
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="inputEmail4">Nombre del propietario</label>
                <input type="text" class="form-control" id="inputEmail4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputState">Marca</label>
                <select id="inputState" class="form-control">
                    <option selected></option>
                    <option>Mercedez Benz</option>
                    <option>Volvo</option>
                </select>
            </div>
            <div class="form-group col-md-4">
                <label for="inputState">Modelo</label>
                <select id="inputState" class="form-control">
                    <option selected></option>
                    <option>...</option>
                </select>
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="inputEmail4">Serie</label>
                <input type="text" class="form-control" id="inputEmail4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Cilindros</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Tipo de combustible</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="inputEmail4">Tipo de servicios</label>
                <input type="text" class="form-control" id="inputEmail4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Numero de motor</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Documentos Legales</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Tipo de llantas</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Capacidad de personas</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Capacidad de toneladas</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Placas</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Oficina expendidora</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
            <div class="form-group col-md-4">
                <label for="inputPassword4">Numero del registro</label>
                <input type="text" class="form-control" id="inputPassword4">
            </div>
        </div>
        <button type="submit" class="btn btn-primary">Guardar</button>
</form>



@page
@model EnergyUI.Pages.Containers.EditVehiclesModel
@{
    ViewData["Title"] = "EditVehicles";
}


<h1>Editar Vehiculos</h1>
<form class="mt-3" method="post">
    <div class="d-flex justify-content-end p-2">
        <button type="submit" class="btn btn-primary m-2">Actualizar</button>
        <a asp-page="/Containers/ReportesCR" class="btn btn-secondary m-2">Cancelar</a>
    </div>
    <input hidden asp-for="Vehicles.Id" />
    <div class="form-group">
        <label></label>
        <input type="text" class="form-control" asp-for="Vehicles.NProp" />
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.Brand" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.NSerie" />
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.Model" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.TipServ" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.NMotor" />
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.TComb" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.DocLeg" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.TipLlantas" />
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.CapPerson" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.CapTon" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.Placas" />
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.OfExp" />
        </div>
        <div class="form-group col-md-6">
            <label></label>
            <input type="text" class="form-control" asp-for="Vehicles.NRegist" />
        </div>
    </div>
</form>

